    "date-fns": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.60.0",
    "react-router": "^7.6.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.75"
